<div class="container">
    <h1>Add your bookmark</h1>
    <%= form_with(model: [@bookmark, @list], url: list_path(@list)) do |form| %>
    <%= form.label :comment %>
    <%= form.text_field :comment %>
    <%= form.submit 'Add a bookmark' %>
  <% end %>
  <%= link_to "Back", lists_path, class: "btn btn-ghost"  %>
</div>
