<div class="container">
  <h1><%= @list.name %></h1>
  <% @movies.each do |movie| %>
      <h1><%= movie.title %></h1>
      <p><%= movie.overview %></p>
  <% end %>
  <%= link_to "Back", lists_path, class: "btn btn-ghost" %>
  <%= link_to "Add a bookmark", new_list_bookmark_path(@list), class: "btn btn-ghost" %>
</div>
